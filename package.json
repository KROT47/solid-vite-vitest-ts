{
  "name": "ui",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "node scripts/add-ts-nocheck.cjs",
    "start": "solid-start dev",
    "build": "solid-start build",
    "build:icons": "./scripts/build-icons/build.sh",
    "serve": "solid-start start",
    "start:prod": "npm run build && npm run serve",
    "test": "vitest",
    "test:run": "vitest run",
    "check": "tsc --noEmit",
    "coverage": "vitest --coverage",
    "format": "prettier --ignore-path .gitignore --write \"**/*.(js|jsx|ts|tsx|json)\"",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix:all": "npm run lint:fix && npm run format"
  },
  "type": "module",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitest/coverage-c8": "^0.25.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-boundaries": "^3.0.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-solid": "^0.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "solid-start-node": "^0.2.0",
    "tsc-files": "^1.1.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "@hope-ui/core": "^1.0.0-next.7",
    "@modular-forms/solid": "^0.8.1",
    "@solid-primitives/i18n": "^1.1.2",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.0",
    "@stitches/core": "^1.2.8",
    "@supabase/supabase-js": "^2.1.0",
    "@tanstack/solid-query": "^4.15.1",
    "clsx": "^1.2.1",
    "effector": "^22.4.0",
    "effector-solid": "^0.22.6",
    "happy-dom": "^7.6.7",
    "solid-icons": "^1.0.4",
    "solid-js": "^1.6.0",
    "solid-start": "^0.2.0",
    "solid-supabase": "^0.3.2",
    "solid-testing-library": "^0.5.0",
    "undici": "^5.11.0"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.(js|jsx|ts|tsx|json)": "prettier --write"
  }
}
